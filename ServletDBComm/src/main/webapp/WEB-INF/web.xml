<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  
  <servlet>
    <description></description>
    <display-name>DBCommServlet</display-name>
    <servlet-name>DBCommServlet</servlet-name>
    <servlet-class>com.in.ineuron.DBCommServlet</servlet-class>
    <init-param>
    	<param-name>url</param-name>
    	<param-value>jdbc:mysql:///telusko</param-value>
    </init-param>
    <init-param>
    	<param-name>username</param-name>
    	<param-value>root</param-value>
    </init-param>
    <init-param>
    	<param-name>password</param-name>
    	<param-value>root</param-value>
    </init-param>
    <load-on-startup>10</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>DBCommServlet</servlet-name>
    <url-pattern>/DBComm</url-pattern>
  </servlet-mapping>
</web-app>